name: On pull request secret tester
on:
  pull_request:

jobs:
  build:
    name: Build and Analyze
    runs-on: ubuntu-22.04
    steps:
      - name: Expose Dummy Secret
        run: |
          DUMMY_SECRET="${{ secrets.DUMMY }}"
          echo "Dummy Secret: $(echo $DUMMY_SECRET | sed 's/./& /g')"
